    "coveralls": "^3.0.0",
    "nyc": "^11.4.1",
    "tape": "^4.8.0",
    "xo": "^0.18.2"
  },
  "dependencies": {
    "clone": "^2.1.1",
    "vfile": "^2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrzmmr/vfile-update.git"
  },
  "bugs": {
    "url": "https://github.com/mrzmmr/vfile-update/issues"
  },
  "homepage": "https://github.com/mrzmmr/vfile-update#readme"
